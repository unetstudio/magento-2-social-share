{
  "name": "unetstudio/magento-2-social-share",
  "description": "Social share toolbar for Magento 2 site",
  "type": "magento2-module",
  "version": "1.0.9",
  "homepage": "https://github.com/unetstudio/magento-2-social-share",
  "authors": [
    {
      "name": "Duc Dao",
      "email": "huuduc.uneti@gmail.com",
      "homepage": "http://newbie-dev.net"
    }
  ],
  "license": ["GNU"],
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "require": {
    "php": "~5.6.0|~7.0.0",
    "magento/framework": "~100.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "phpmd/phpmd": "^2.3",
    "sebastian/phpcpd": "^2.0",
    "satooshi/php-coveralls": "^1.0",
    "friendsofphp/php-cs-fixer": "^2.2",
    "squizlabs/php_codesniffer": "*",
    "magento-ecg/coding-standard": "^3.1"
  },
  "scripts": {
    "test": [
      "phpunit --testdox",
      "phpcs"
    ],
    "ecg": "php vendor/bin/phpcs --standard=./vendor/magento-ecg/coding-standard/EcgM2 Adapter/ Block/ Helper/ Model/ Test/"
  },
  "autoload": {
    "files": [ "registration.php" ],
    "psr-4": {
      "Unet\\SocialShare\\": ""
    }
  }
}
